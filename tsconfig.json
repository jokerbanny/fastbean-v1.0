{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "lib": ["ES6"],
    "allowJs": true,
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": ["src", "./src/**/*.ts"],
  "exclude": ["node_modules", "frontend", "drizzle.config.ts", "save"]
}
